{
    "name": "mern-testing-assignment",
    "version": "1.0.0",
    "description": "MERN Stack Testing and Debugging Assignment",
    "scripts": {
      "dev": "concurrently \"npm run server\" \"npm run client\"",
      "server": "cd server && npm run dev",
      "client": "cd client && npm run dev",
      "test": "cd server && npm test",
      "test:server": "cd server && npm test",
      "test:coverage": "cd server && npm run test:coverage",
      "test:integration": "cd server && npm run test:integration",
      "install-all": "npm install && cd server && npm install && cd ../client && npm install"
    },
    "devDependencies": {
      "concurrently": "^7.6.0",
      "jest": "^29.0.0"
    },
    "keywords": ["mern", "testing", "debugging", "jest"],
    "author": "Student",
    "license": "MIT"
  }